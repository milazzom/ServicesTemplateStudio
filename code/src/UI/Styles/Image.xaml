<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:strings="clr-namespace:Microsoft.Templates.UI.Resources" >

    <Style TargetType="Image" x:Key="WtsImageNewItemFileIcon">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.BitmapScalingMode" Value="NearestNeighbor"/>
        <Setter Property="AutomationProperties.Name" Value="{x:Static strings:StringRes.AccessibilityNewItemFileIcon}"/>
    </Style>

    <Style TargetType="Image" x:Key="WtsImageNewItemImageFiles">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Stretch" Value="None"/>
        <Setter Property="RenderOptions.BitmapScalingMode" Value="NearestNeighbor"/>
    </Style>

</ResourceDictionary>
