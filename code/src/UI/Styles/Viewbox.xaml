<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:services="clr-namespace:Microsoft.Templates.UI.Services">

    <Style TargetType="Viewbox" x:Key="WtsViewBoxDialogIcon">
        <Setter Property="Width" Value="40" />
        <Setter Property="Height" Value="40" />
        <Setter Property="StretchDirection" Value="Both" />
        <Setter Property="VerticalAlignment" Value="Top" />
    </Style>

    <Style TargetType="Viewbox" x:Key="WtsViewBoxNotificationIcon">
        <Setter Property="Width" Value="20" />
        <Setter Property="Height" Value="20" />
        <Setter Property="StretchDirection" Value="Both" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <Style TargetType="Viewbox" x:Key="WtsViewBoxCloseIcon">
        <Setter Property="Width" Value="14" />
        <Setter Property="Height" Value="14" />
        <Setter Property="StretchDirection" Value="Both" />
    </Style>

</ResourceDictionary>
