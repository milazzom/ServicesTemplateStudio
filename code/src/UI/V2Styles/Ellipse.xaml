<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:services="clr-namespace:Microsoft.Templates.UI.V2Services">

    <Style TargetType="Ellipse" x:Key="WtsEllipseNewItemFileCircle">
        <Setter Property="Width" Value="10" />
        <Setter Property="Height" Value="10" />
        <Setter Property="StrokeThickness" Value="1" />
        <Setter Property="Stroke" Value="{Binding WindowPanel, Source={x:Static services:UIStylesService.Instance}}" />
    </Style>
    
</ResourceDictionary>
