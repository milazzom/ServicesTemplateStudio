<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:colors="clr-namespace:Microsoft.Templates.VsEmulator.Services">

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.GridHeadingBackgroundBrushKey}" Color="#00000000" />
    <Color x:Key="{x:Static colors:ThemedDialog.GridHeadingBackgroundColorKey}" A="0" R="0" G="0" B="0" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.GridHeadingHoverBackgroundBrushKey}" Color="#00000000" />
    <Color x:Key="{x:Static colors:ThemedDialog.GridHeadingHoverBackgroundColorKey}" A="0" R="0" G="0" B="0" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.GridHeadingHoverTextBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.GridHeadingHoverTextColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.GridHeadingTextBrushKey}" Color="#FF717171" />
    <Color x:Key="{x:Static colors:ThemedDialog.GridHeadingTextColorKey}" A="255" R="113" G="113" B="113" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.GridLineBrushKey}" Color="#FFCCCEDB" />
    <Color x:Key="{x:Static colors:ThemedDialog.GridLineColorKey}" A="255" R="204" G="206" B="219" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.HeaderTextBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.HeaderTextColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.HyperlinkBrushKey}" Color="#FF0E70C0" />
    <Color x:Key="{x:Static colors:ThemedDialog.HyperlinkColorKey}" A="255" R="14" G="112" B="192" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.HyperlinkDisabledBrushKey}" Color="#FFA2A4A5" />
    <Color x:Key="{x:Static colors:ThemedDialog.HyperlinkDisabledColorKey}" A="255" R="162" G="164" B="165" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.HyperlinkHoverBrushKey}" Color="#FF007ACC" />
    <Color x:Key="{x:Static colors:ThemedDialog.HyperlinkHoverColorKey}" A="255" R="0" G="122" B="204" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.HyperlinkPressedBrushKey}" Color="#FF0E70C0" />
    <Color x:Key="{x:Static colors:ThemedDialog.HyperlinkPressedColorKey}" A="255" R="14" G="112" B="192" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.ListItemDisabledTextBrushKey}" Color="#FFA2A4A5" />
    <Color x:Key="{x:Static colors:ThemedDialog.ListItemDisabledTextColorKey}" A="255" R="162" G="164" B="165" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.ListItemMouseOverBrushKey}" Color="#FFC9DEF5" />
    <Color x:Key="{x:Static colors:ThemedDialog.ListItemMouseOverColorKey}" A="255" R="201" G="222" B="245" />
    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.ListItemMouseOverTextBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.ListItemMouseOverTextColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.SectionDividerBrushKey}" Color="#FFCCCEDB" />
    <Color x:Key="{x:Static colors:ThemedDialog.SectionDividerColorKey}" A="255" R="204" G="206" B="219" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.SelectedItemActiveBrushKey}" Color="#FF007ACC" />
    <Color x:Key="{x:Static colors:ThemedDialog.SelectedItemActiveColorKey}" A="255" R="0" G="122" B="204" />
    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.SelectedItemActiveTextBrushKey}" Color="#FFFFFFFF" />
    <Color x:Key="{x:Static colors:ThemedDialog.SelectedItemActiveTextColorKey}" A="255" R="255" G="255" B="255" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.SelectedItemInactiveBrushKey}" Color="#FFCCCEDB" />
