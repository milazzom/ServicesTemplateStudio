    <Color x:Key="{x:Static colors:ThemedDialog.SelectedItemInactiveTextColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowBorderBrushKey}" Color="#FFCCCEDB" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowBorderColorKey}" A="255" R="204" G="206" B="219" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonBrushKey}" Color="#00000000" />
