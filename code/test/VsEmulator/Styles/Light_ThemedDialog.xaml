    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonDownBorderColorKey}" A="255" R="0" G="122" B="204" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonDownGlyphBrushKey}" Color="#FFFFFFFF" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonDownGlyphColorKey}" A="255" R="255" G="255" B="255" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonGlyphBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonGlyphColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverBrushKey}" Color="#FFC9DEF5" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverColorKey}" A="255" R="201" G="222" B="245" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverBorderBrushKey}" Color="#FFC9DEF5" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverBorderColorKey}" A="255" R="201" G="222" B="245" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverGlyphBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowButtonHoverGlyphColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowPanelBrushKey}" Color="#FFFFFFFF" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowPanelColorKey}" A="255" R="255" G="255" B="255" />
    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WindowPanelTextBrushKey}" Color="#FF1E1E1E" />
    <Color x:Key="{x:Static colors:ThemedDialog.WindowPanelTextColorKey}" A="255" R="30" G="30" B="30" />

    <SolidColorBrush x:Key="{x:Static colors:ThemedDialog.WizardFooterBrushKey}" Color="#FFEFEFF2" />
    <Color x:Key="{x:Static colors:ThemedDialog.WizardFooterColorKey}" A="255" R="239" G="239" B="242" />
